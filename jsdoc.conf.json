{
    "plugins": [
        "node_modules/better-docs/typescript"
    ],
    "recurseDepth": 10,
    "source": {
        "include": [
            "src"
        ],
        "includePattern": ".+\\.(js|ts)$"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": [
            "jsdoc",
            "closure"
        ]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "destination": "./doc/",
        "template": "node_modules/better-docs"
    }
}